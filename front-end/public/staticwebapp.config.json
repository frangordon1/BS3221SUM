{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "clientId": "your-client-id",
          "openIdIssuer": "your-issuer-url"
        }
      }
    }
  }
}
